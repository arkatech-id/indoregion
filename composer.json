{
    "name": "arkatech-id/indoregion",
    "description": "Package containing region of Indonesia (provinces, regencies, districts, and villages) for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "php",
        "csv",
        "indonesia",
        "provinces",
        "regencies",
        "districts",
        "villages",
        "region"
    ],
    "authors": [
        {
            "name": "arkatech.id",
            "email": "arkatech.id@gmail.com"
        }
    ],
    "require": {
        "parsecsv/php-parsecsv": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "ArkaTech\\IndoRegion\\": "src/"
        },
        "files": [
            "src/RawDataGetter.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArkaTech\\IndoRegion\\IndoRegionServiceProvider"
            ],
            "aliases": {}
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
